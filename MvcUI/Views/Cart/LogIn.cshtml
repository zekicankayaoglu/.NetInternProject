@model LoggingDetailsViewModel

<h2>Log In</h2>
<form asp-controller="Cart" asp-action="LogIn" method="post">
    
    <div class="form-group">
        <label asp-for="LoggingDetails.Email">Email</label>
        <input asp-for="LoggingDetails.Email" class="form-control" placeholder="Email of Customer" />
        <span asp-validation-for="LoggingDetails.Email"></span>
    </div>
    <div class="form-group">
        <label asp-for="LoggingDetails.Password">Password</label>
        <input asp-for="LoggingDetails.Password" class="form-control" placeholder="Password of Customer" />
        <span asp-validation-for="LoggingDetails.Password"></span>
    </div>
   
    <input type="submit" value="Log In" class="btn btn-success btn-group-sm" />
</form>
